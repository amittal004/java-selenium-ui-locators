{
    "manifest_version": 3,
    "name": "Locator Extension",
    "version": "1.0",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon16.png"
        }
    }
}
